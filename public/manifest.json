{
  "manifest_version": 3,
  "name": "Chat GPT Prompts",
  "version": "0.1.0",
  "description": "EXPERIMENTAL WITH VITE",
  "action": {
    "default_popup": "src/popup/index.html"
  },
  "background": {
    "service_worker": "service-worker.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["content-script.js"]
    }
  ],
  "permissions": ["scripting"],
  "host_permissions": ["http://*/*", "https://*/*"]
}